        <TextBox Height="23" HorizontalAlignment="Left" Margin="146,52,0,0" Name="widthText" VerticalAlignment="Top" Width="120" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="146,121,0,0" Name="heightText" VerticalAlignment="Top" Width="120" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="39,52,0,0" Name="widthTextLab" Text="Width" VerticalAlignment="Top" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="39,124,0,0" Name="heightTextLab" Text="Height" VerticalAlignment="Top" />
