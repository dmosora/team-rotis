<Window x:Class="SampleCode.nameChanger" Keyboard.KeyDown="Window_KeyDown"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Enter Layer Name" Height="120" Width="220" WindowStyle="ToolWindow">
    <Grid>
        <Button Content="Okay" Height="23" HorizontalAlignment="Left" Margin="12,46,0,0" Name="okBut" VerticalAlignment="Top" Width="75" Click="okBut_Click" TabIndex="1" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="111,46,0,0" Name="cancelBut" VerticalAlignment="Top" Width="75" Click="cancelBut_Click" TabIndex="2" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="39,12,0,0" Name="nameText" VerticalAlignment="Top" Width="120" TabIndex="0" />
    </Grid>
</Window>
